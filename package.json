{
  "name": "boilerplate",
  "version": "3.1.0",
  "description": "remixed and reduced ds boilerplate",
  "scripts": {
    "precommit": "lint-staged",
    "dev": "npm run webpack -- --env.mode development --env.NODE_ENV='local' --watch --content-base public_html/",
    "prod": "npm run webpack -- --env.mode production --env.NODE_ENV='production' ",
    "prod:debug": "npm run debug -- --env.mode production",
    "dev:debug": "npm run debug -- --env.mode development",
    "prod:typescript": "npm run prod -- --env.presets typescript",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js"
  },
  "lint-staged": {
    "{,!(node_modules)/}**/*.{js,jsx,css}": [
      "prettier --write --no-semi --single-quote --jsx-bracket-same-line",
      "git add"
    ]
  },
  "browserslist": [
    "> 5%",
    "maintained node versions",
    "not dead",
    "ie 11"
  ],
  "author": "Digital Surgeons",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "acorn": "^6.1.1",
    "autoprefixer": "~7.2.6",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "glob": "~7.1.3",
    "husky": "^0.14.3",
    "lint-staged": "~4.3.0",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-js": "^1.0.1",
    "postcss-cssnext": "~2.11.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^2.1.6",
    "postcss-normalize": "^7.0.1",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-preset-env": "^6.6.0",
    "prettier": "~1.14.3",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^1.0.0",
    "stylelint-order": "^0.6.0",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "svg-sprite-generator": "0.0.7",
    "url-loader": "^0.5.9",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1",
    "webpack-notifier": "~1.6.0"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "headroom.js": "^0.9.4",
    "jquery": "~3.3.1",
    "normalize.css": "^5.0.0",
    "object-fit-images": "~3.2.4",
    "postcss": "~7.0.4",
    "postcss-scss": "~2.0.0",
    "postcss-strip-inline-comments": "~0.1.5",
    "precss": "~3.1.2",
    "svgxuse": "~1.2.6"
  },
  "resolutions": {
    "webpack/acorn": "6.1.1"
  }
}
