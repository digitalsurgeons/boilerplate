{
  "name": "boilerplate",
  "version": "2.0.0",
  "description": "remixed and reduced ds boilerplate",
  "main": "public_html/index.html",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "webpack:dev": "SITE=$site webpack --colors --watch",
    "webpack:prod": "webpack --config ./webpack.config.prod.js --progress --colors",
    "prestart": "NODE_ENV=production npm run webpack:prod",
    "start": "ecstatic --root public_html --cache 0 --port 8080",
    "dev": "concurrently --kill-others 'ecstatic --root public_html --cache 0 --port 8080' 'npm run webpack:dev'",
    "eslint": "eslint **/*.js"
  },
  "author": "Digital Surgeons",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.16.0",
    "browser-sync-webpack-plugin": "^1.1.3",
    "browserify": "^13.1.0",
    "clean-webpack-plugin": "^0.1.14",
    "compiler-webpack-plugin": "0.0.1",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "ds-css-cli": "^1.0.14",
    "ecstatic": "^2.1.0",
    "eslint": "^3.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "factor-bundle": "^2.5.0",
    "fastclick": "^1.0.6",
    "file-loader": "^0.9.0",
    "jquery": "^3.1.0",
    "node-sass": "^3.10.0",
    "normalize.css": "^5.0.0",
    "onchange": "^3.0.2",
    "optimize-js": "^1.0.1",
    "postcss-emify": "^1.0.2",
    "postcss-loader": "^1.1.1",
    "postcss-remify": "^1.0.1",
    "sass-lint": "^1.9.1",
    "sass-loader": "^4.0.2",
    "svg-sprite-generator": "0.0.7",
    "svgxuse": "^1.1.23",
    "uglify-js": "^2.7.3",
    "uglifyify": "^3.0.3",
    "url-loader": "^0.5.7",
    "watchify": "^3.7.0",
    "webpack": "^1.13.3",
    "webpack-notifier": "^1.4.1"
  }
}
