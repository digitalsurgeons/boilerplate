{
  "name": "boilerplate",
  "version": "3.1.0",
  "description": "remixed and reduced ds boilerplate",
  "scripts": {
    "dev": "npm run webpack -- --env.mode development --env.NODE_ENV='local' --watch --content-base public_html/",
    "prod": "npm run webpack -- --env.mode production --env.NODE_ENV='production' ",
    "prod:debug": "npm run debug -- --env.mode production",
    "dev:debug": "npm run debug -- --env.mode development",
    "prod:typescript": "npm run prod -- --env.presets typescript",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js"
  },
  "browserslist": [
    "> 5%",
    "maintained node versions",
    "not dead",
    "ie 11"
  ],
  "author": "Digital Surgeons",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "acorn": "^6.1.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "glob": "^7.1.3",
    "mini-css-extract-plugin": "^0.6.0",
    "object-fit-images": "^3.2.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-normalize": "^7.0.1",
    "postcss-preset-env": "^6.6.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.18.2",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {},
  "resolutions": {
    "webpack/acorn": "6.1.1"
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
