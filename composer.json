{
  "name": "craftcms/craft",
  "description": "Craft CMS",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "license": "MIT",
  "homepage": "https://craftcms.com/",
  "type": "project",
  "support": {
    "email": "support@craftcms.com",
    "issues": "https://github.com/craftcms/cms/issues",
    "forum": "https://craftcms.stackexchange.com/",
    "source": "https://github.com/craftcms/cms",
    "docs": "https://craftcms.com/docs",
    "rss": "https://craftcms.com/changelog.rss"
  },
  "require": {
    "craftcms/cms": "3.1.21.1",
    "vlucas/phpdotenv": "^2.4.0",
    "verbb/super-table": "2.1.16",
    "charliedev/element-map": "1.0.2",
    "mmikkel/cp-field-inspect": "1.0.5",
    "ether/seo": "3.5.4",
    "ostark/upper": "1.3.2",
    "studioespresso/craft-dumper": "1.3.1",
    "topshelfcraft/environment-label": "3.1.5",
    "rias/craft-position-fieldtype": "1.0.14",
    "nystudio107/craft-pathtools": "1.0.7",
    "aelvan/imager": "v2.0.0",
    "nystudio107/craft-cookies": "1.1.11",
    "marionnewlevant/twig-perversion": "2.0.7",
    "digitalsurgeons/twig-dig": "1.0.0",
    "coryzibell/nanoslugs": "1.0.3",
    "trendyminds/molecule": "1.2.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@composer dump-autoload -o",
      "@php craft setup/welcome"
    ]
  }
}
